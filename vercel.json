

{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { "source": "/admin(.*)", "destination": "/" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/admin.*", "dest": "/" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
